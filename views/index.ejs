<!DOCTYPE html>
<html>
<head>
  <title>Files</title>
  <link rel="stylesheet" type="text/css" href="/styles.css">
</head>
<body>
  <h1>Files List</h1>
  <ul>
    <% files.forEach(file => { %>
      <li><a href="/files/<%= file %>"><%= file %></a></li>
    <% }) %>
  </ul>
  <a href="/create">Create New File</a>
</body>
</html>
